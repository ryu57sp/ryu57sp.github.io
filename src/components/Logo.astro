---
interface Props {
  logo?: string;
}
const { logo = "RM" } = Astro.props;
---

<div class="flex justify-center mb-12 lg:mb-16">
  <div
    class="h-12 w-12 rounded-full bg-gradient-to-r from-white to-yellow-300 flex items-center justify-center"
  >
    <span class="text-black text-xl font-bold">{logo}</span>
  </div>
</div>